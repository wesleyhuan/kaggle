self.fill_values['weight'] = w_kg.median() if self.use_median else w_kg.mean()\n        \n        # Numeric columns\n        numeric_cols = ['s', 'a', 'dir', 'o']\n        for col in numeric_cols:\n            if col in df.columns:\n                 # Force numeric\n                vals = pd.to_numeric(df[col], errors='coerce')\n                self.fill_values[col] = vals.median() if self.use_median else vals.mean()\n                \n        return self\n\n    def transform(self, X):\n        df = X.copy()\n\n        # --- A. Coordinate Standardization (The \"Kaggle Winning\" Step) ---\n        # We want Offense to always move Left -> Right\n        # Assuming 'play_direction' is in the dataframe\n        if 'play_direction' in df.columns:\n            # Mask for plays moving left\n            mask_left = df['play_direction'] == 'left'\n            \n            # Flip X and Y\n            df.loc[mask_left, 'x'] = 120 - df.loc[mask_left, 'x']\n            df.loc[mask_left, 'y'] = 53.3 - df.loc[mask_left, 'y']\n            \n            # Flip Direction/Orientation (math required here)\n            if 'dir' in df.columns:\n                 df.loc[mask_left, 'dir'] = (df.loc[mask_left, 'dir'] + 180) % 360\n            if 'o' in df.columns:\n                 df.loc[mask_left, 'o'] = (df.loc[mask_left, 'o'] + 180) % 360\n\n        # --- B. Feature Engineering & Cleaning ---\n        \n        # Height\n        df['player_height_m'] = df['player_height'].map(to_meters)\n        # FIX: Assign back to column\n        df['player_height_m'] = df['player_height_m'].fillna(self.fill_values['height'])\n\n        # Weight\n        df['player_weight_kg'] = pd.to_numeric(df['player_weight'], errors='coerce') * 0.453592\n        # FIX: Assign back to column\n        df['player_weight_kg'] = df['player_weight_kg'].fillna(self.fill_values['weight'])\n\n        # Age\n        df['player_age'] = df['player_birth_date'].map(to_age)\n        df['player_age'] = df['player_age'].fillna(25) # Default NFL age\n\n        # Apply Label Encoding\n        for col, le in self.label_encoders.items():\n            if col in df.columns:\n                # Handle unseen labels in test data by mapping to a default or handling exception\n                # Simple approach: Convert to str, if not in classes, assign -1 or mode\n                # Here we just do standard transform for simplicity, but be careful in prod\n                df[f\"{col}_encoded\"] = le.transform(df[col].astype(str).fillna(\"NA\"))\n\n        # Numeric fill\n        numeric_cols = ['s', 'a', 'dir', 'o']\n        for col in numeric_cols:\n            if col in df.columns:\n                df[col] = pd.to_numeric(df[col], errors='coerce')\n                # FIX: Assign back\n                df[col] = df[col].fillna(self.fill_values[col])\n\n        return df","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-26T15:54:54.507962Z","iopub.execute_input":"2025-11-26T15:54:54.508501Z","iopub.status.idle":"2025-11-26T15:54:54.520502Z","shell.execute_reply.started":"2025-11-26T15:54:54.508482Z","shell.execute_reply":"2025-11-26T15:54:54.519626Z"}},"outputs":[],"execution_count":40},{"cell_type":"code","source":"# 初始化\npreprocessor = PlayerPreprocessor(use_median=True)\n# fit + transform\ndf_processed = preprocessor.fit_transform(df_in)\nprint(df_processed.head())","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-26T15:54:54.521329Z","iopub.execute_input":"2025-11-26T15:54:54.521730Z","iopub.status.idle":"2025-11-26T15:55:19.716796Z","shell.execute_reply.started":"2025-11-26T15:54:54.521712Z","shell.execute_reply":"2025-11-26T15:55:19.716061Z"}},"outputs":[{"name":"stdout","text":"      game_id  play_id  player_to_predict  nfl_id  frame_id play_direction  \\\n0  2023101900       78              False   54514         1          right   \n1  2023101900       78              False   54514         2          right   \n2  2023101900       78              False   54