tputs = [], []\n    with ThreadPoolExecutor(max_workers=max_workers) as executor:#multi thread\n        futures = {executor.submit(load_week, w): w for w in week_nums}\n        for future in tqdm(as_completed(futures), total=len(futures), desc=\"Loading weeks\"):#- Uses tqdm to show a progress bar as each thread finishes\n            w, inp, out = future.result()\n            inputs.append(inp)\n            outputs.append(out)\n\n    return pd.concat(inputs, ignore_index=True), pd.concat(outputs, ignore_index=True)\n\ndf_in, df_out = load_weeks_parallel(WEEKS, TRAIN_PATH, max_workers=8)\nprint(f\"Data loaded: {len(df_in)} input rows, {df_in['game_id'].nunique()} games\")\nprint(f\"Data loaded: {len(df_out)} output rows, {df_out['game_id'].nunique()} games\")","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-26T15:54:24.528893Z","iopub.execute_input":"2025-11-26T15:54:24.529081Z","iopub.status.idle":"2025-11-26T15:54:29.589630Z","shell.execute_reply.started":"2025-11-26T15:54:24.529067Z","shell.execute_reply":"2025-11-26T15:54:29.588800Z"}},"outputs":[{"name":"stderr","text":"Loading weeks: 100%|██████████| 11/11 [00:04<00:00,  2.42it/s]\n","output_type":"stream"},{"name":"stdout","text":"Data loaded: 2941176 input rows, 164 games\nData loaded: 336358 output rows, 164 games\n","output_type":"stream"}],"execution_count":26},{"cell_type":"code","source":"print(df_in.shape)\nprint(df_out.shape)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-26T15:54:29.590560Z","iopub.execute_input":"2025-11-26T15:54:29.590889Z","iopub.status.idle":"2025-11-26T15:54:29.595894Z","shell.execute_reply.started":"2025-11-26T15:54:29.590862Z","shell.execute_reply":"2025-11-26T15:54:29.595056Z"}},"outputs":[{"name":"stdout","text":"(2941176, 23)\n(336358, 6)\n","output_type":"stream"}],"execution_count":27},{"cell_type":"markdown","source":"# Check data for basic info\nCheck what data we are about to handle to know what we should do to preprocess data