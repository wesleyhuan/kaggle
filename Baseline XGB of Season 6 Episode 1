    "# 假設你跑完了兩個模型的 run_cv_and_predict\n",
    "# run OOF with best parameter\n",
    "xgb_best_params = XGB_study.best_params\n",
    "oof_train, xgb_test_preds = run_cv_and_predict(X_train_raw, y_train, test, xgb_best_params)\n",
    "\n",
    "lgbm_best_params = lgbm_study.best_params\n",
    "oof_train, lgbm_test_preds = run_cv_and_predict(X_train_raw, y_train, test, lgbm_best_params)\n",
    "\n",
    "# simple 50% 50% prediction\n",
    "final_submission_preds = (0.5 * xgb_test_preds) + (0.5 * lgbm_test_preds)\n",
    "\n",
    "submission = pd.DataFrame({\n",
    "    'id': test['id'],\n",
    "    'exam_score': final_submission_preds\n",
    "})\n",
    "submission.to_csv('ensemble_submission.csv', index=False)"
   ]
