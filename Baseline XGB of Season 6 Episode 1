   "source": [
    "def lgbm_objective(trial):\n",
    "    params = {\n",
    "        \"n_estimators\": 2000,\n",
    "        \"learning_rate\": trial.suggest_float(\"learning_rate\", 0.005, 0.1, log=True),\n",
    "        \"num_leaves\": trial.suggest_int(\"num_leaves\", 20, 3000), # LGBM 核心參數\n",
    "        \"max_depth\": trial.suggest_int(\"max_depth\", 3, 12),\n",
    "        \"min_child_samples\": trial.suggest_int(\"min_child_samples\", 5, 100), # 類似 min_child_weight\n",
    "        \"subsample\": trial.suggest_float(\"subsample\", 0.5, 1.0),\n",
    "        \"colsample_bytree\": trial.suggest_float(\"colsample_bytree\", 0.5, 1.0),\n",
    "        \"reg_alpha\": trial.suggest_float(\"reg_alpha\", 1e-8, 10.0, log=True),\n",
    "        \"reg_lambda\": trial.suggest_float(\"reg_lambda\", 1e-8, 10.0, log=True),\n",
    "        \"verbosity\": -1 # 減少輸出的訊息\n",
    "    }\n",
    "    \n",
    "    # 這裡同樣可以使用我們之前寫的 run_cv_and_predict 邏輯\n",
    "    # 或者簡單使用 cross_val_score\n",
    "    pipeline = Pipeline(steps=[\n",
    "        (\"preprocess\", Preprocessor()),\n",
    "        (\"model\", LGBMRegressor(**params, random_state=CFG.RANDOM_SEED))\n",
    "    ])\n",
    "    \n",
    "    scores = cross_val_score(\n",
    "        pipeline, X_train_raw, y_train, \n",
    "        cv=CFG.N_FOLDS, \n",
    "        scoring=\"neg_root_mean_squared_error\"\n",
    "    )\n",
    "    return -scores.mean()"
   ]
  },
